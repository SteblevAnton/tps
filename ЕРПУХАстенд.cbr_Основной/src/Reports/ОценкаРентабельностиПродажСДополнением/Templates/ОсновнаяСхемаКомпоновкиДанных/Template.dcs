			<role>
				<dcscom:dimension>true</dcscom:dimension>
			</role>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>ХарактеристикаАВ</dataPath>
			<field>Характеристика</field>
			<title xsi:type="v8:LocalStringType">
				<v8:item>
					<v8:lang>ru</v8:lang>
					<v8:content>Характеристика</v8:content>
				</v8:item>
			</title>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>СтавкаАгентаАВ</dataPath>
			<field>СтавкаАгента</field>
			<title xsi:type="v8:LocalStringType">
				<v8:item>
					<v8:lang>ru</v8:lang>
					<v8:content>Ставка, %</v8:content>
				</v8:item>
			</title>
		</field>
		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ
	ЗаказКлиента.Организация КАК Организация,
	ЗаказКлиента.Контрагент КАК Контрагент,
	ЗаказКлиентаТовары.Номенклатура КАК Номенклатура,
	ЗаказКлиентаТовары.Характеристика КАК Характеристика
ПОМЕСТИТЬ ВТ_ЗаказКлиента
{ВЫБРАТЬ
	Организация.*,
	Контрагент.*}
ИЗ
	Документ.ЗаказКлиента.Товары КАК ЗаказКлиентаТовары
		ВНУТРЕННЕЕ СОЕДИНЕНИЕ Документ.ЗаказКлиента КАК ЗаказКлиента
		ПО ЗаказКлиентаТовары.Ссылка = ЗаказКлиента.Ссылка
ГДЕ
	ТИПЗНАЧЕНИЯ(&amp;Документ) = ТИП(Документ.ЗаказКлиента)
	И ЗаказКлиента.Ссылка = &amp;Документ

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	КоммерческоеПредложениеКлиенту.Организация,
	КоммерческоеПредложениеКлиенту.Контрагент,
	КоммерческоеПредложениеКлиентуТовары.Номенклатура,
	КоммерческоеПредложениеКлиентуТовары.Характеристика
ИЗ
	Документ.КоммерческоеПредложениеКлиенту.Товары КАК КоммерческоеПредложениеКлиентуТовары
		ВНУТРЕННЕЕ СОЕДИНЕНИЕ Документ.КоммерческоеПредложениеКлиенту КАК КоммерческоеПредложениеКлиенту
		ПО КоммерческоеПредложениеКлиентуТовары.Ссылка = КоммерческоеПредложениеКлиенту.Ссылка
ГДЕ
	ТИПЗНАЧЕНИЯ(&amp;Документ) = ТИП(Документ.КоммерческоеПредложениеКлиенту)
	И КоммерческоеПредложениеКлиенту.Ссылка = &amp;Документ
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	УсловияАгентскогоВознагражденияСрезПоследних.Регистратор КАК УсловияАВ,
	УсловияАгентскогоВознагражденияСрезПоследних.Агент КАК Агент,
	УсловияАгентскогоВознагражденияСрезПоследних.Номенклатура КАК Номенклатура,
	УсловияАгентскогоВознагражденияСрезПоследних.Характеристика КАК Характеристика,
	ЕСТЬNULL(УсловияАгентскогоВознагражденияСрезПоследних.СтавкаАгента, 0) КАК СтавкаАгента
ПОМЕСТИТЬ ВТ_УсловияАВПоЗаказуПредварительно
{ВЫБРАТЬ
	УсловияАВ.*}
ИЗ
	РегистрСведений.УсловияАгентскогоВознаграждения.СрезПоследних(
			,
			Основание = &amp;Документ
				И (Организация, Клиент, Номенклатура, Характеристика) В
					(ВЫБРАТЬ
						Т.Организация,
						Т.Контрагент,
						Т.Номенклатура,
						Т.Характеристика
					ИЗ
						ВТ_ЗаказКлиента КАК Т)) КАК УсловияАгентскогоВознагражденияСрезПоследних
;

////////////////////////////////////////////////////////////////////////////////
