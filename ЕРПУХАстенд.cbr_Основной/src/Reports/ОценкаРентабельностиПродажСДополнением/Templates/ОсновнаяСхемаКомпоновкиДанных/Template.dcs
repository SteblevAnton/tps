				<v8:item>
					<v8:lang>ru</v8:lang>
					<v8:content>Условия АВ</v8:content>
				</v8:item>
			</title>
			<role>
				<dcscom:required>true</dcscom:required>
			</role>
		</field>
		<field xsi:type="DataSetFieldField">
